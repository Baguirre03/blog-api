{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-use-before-define": "off",
        "no-unused-vars": "off",
        "no-underscore-dangle": "off",
        "func-names": "off"
    }
}
